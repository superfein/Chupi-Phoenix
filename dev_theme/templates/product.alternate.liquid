<script type="text/javascript">
    if ($("#videoJSON").val()){
        var video_val = $("#videoJSON").val().replace(/'/g, "\"");
        var asset_url = $("#videoJSON").data("asset-url");
        var video_json = JSON.parse(video_val);
        var cloned_elements = "";
        for (var i = 0; i < video_json.length; i++) {
          var obj = video_json[i];
          var poster = asset_url.replace(/&/g, obj.image);
          var src = asset_url.replace(/&/g, obj.video);
          cloned_elements += "<video controls loop poster=" + poster + "><source src=" + src + " type=\"video/mp4\"></video>";
        }
        $("#videos").html(cloned_elements);
      }
</script>
<script text="text/javascript">
  var _learnq = _learnq || [];

  var item = {
    Name: {{ product.title|json }},
    ProductID: {{ product.id|json }},
    Categories: {{ product.collections|map:'title'|json }},
    ImageURL: "https:{{ product.featured_image.src|img_url:'grande' }}",
    URL: "{{ shop.secure_url }}{{ product.url }}",
    Brand: {{ product.vendor|json }},
    Price: {{ product.price|money|json }},
    CompareAtPrice: {{ product.compare_at_price_max|money|json }}
  };

  _learnq.push(['track', 'Viewed Product', item]);
  _learnq.push(['trackViewedItem', {
    Title: item.Name,
    ItemId: item.ProductID,
    Categories: item.Categories,
    ImageUrl: item.ImageURL,
    Url: item.URL,
    Metadata: {
      Brand: item.Brand,
      Price: item.Price,
      CompareAtPrice: item.CompareAtPrice
    }
  }]);
</script>
