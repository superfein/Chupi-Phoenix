{% if product.type == 'SHOPSTORM_HIDDEN_PRODUCT' %} Product is not available {% break %} {% endif %}

<!-- Structured Data -->
<div>
  <div itemtype="http://schema.org/Product" itemscope>
    <meta itemprop="mpn" content="{{ product.id }}" />
    <meta itemprop="name" content="{{ product.title }}" />
    <link itemprop="image" href="{{ product | img_url }}" />
    <div itemprop="brand" itemtype="http://schema.org/Thing" itemscope>
      <meta itemprop="name" content="{{ shop.name }}" />
    </div>
    <div itemprop="offers" itemtype="http://schema.org/Offer" itemscope>
      <meta itemprop="priceCurrency" content="{{ cart.currency.iso_code }}">
      <meta itemprop="price" content="{{ product.price | divided_by: 100}}">
    </div>
  </div>
</div>
<!--main start -->
<div id="main-wrap">
    <div id="main" class="clearfix layout-setting">


      	<!-- Product Upper -->
      <section id="product-upper">
        <div class="container">
          <div class="row">

            <!-- Breadcrumbs -->
            <!--
            <div class="col-md-12 product-breadcrumbs">
              <a href="/">Home</a>
              <a href="javascript:;"></a>
              <a href="javascript:;"></a>
              <span>{{ product.title }}</span>
            </div>
			-->


      <!-- Product Gallery -->
                  <div class="col-xs-12 col-md-7 col-lg-8 product-gallery">
                    <div class="thumbnails">
                      <!-- Thumbnails dynamically created with JS -->
                    </div>
                    <div class="display-box">
                      <!-- Gallery images go here -->
                      {% for image in product.images %}
                          <a href="{{image.src | img_url: '1024x1024'}}" title="{{ image.alt | escape }}" data-lightbox="product-set" data-title="" style="background-image: url('{{image.src | img_url: '1024x1024'}}');"></a>
                      {% endfor %}
      <!--                 <input type="hidden" id="videoJSON" value="{{ product.metafields.product_media.video }}" data-asset-url="{{ '&' | asset_url }}" /> -->
                      <div id="videos">
                        <video class="product-video" src="" loop autoplay></video>
                      </div>
                    </div>
                  </div> <!-- end .product-gallery -->


                  <!-- March 2018 -->
                  <!-- Product Gallery Mobile -->
                  <div class="col-xs-12 product-gallery-mobile">

                    <div class="product-slider">
                      <ul class="bxslider2">
                        {% for image in product.images %}
                <!--	Gets rid of video placeholder	-->
                          {% if image.alt contains '.m4v' %}
                          {% else %}
                            <li>
                              <a href="{{image.src | img_url: '1024x1024'}}" class="colorbox-gallery" onclick="return false;">
                                <img src="{{image.src | img_url: '1024x1024'}}" alt="image-{{ image.alt }}">
                              </a>
                            </li>
                          {% endif %}
                        {% endfor %}
                      </ul>

                      {% if product.images.size <=50 %}
                        <div id="bx-pager">
                          {% for image in product.images %}
      <!--                     removes video file extension from pager icons  -->
                            {% if image.alt contains '.m4v' %}
                            {% else %}
                              <a data-slide-index="{{forloop.index0}}" href=""><img src="{{image.src | img_url: 'thumb'}}" alt="image-{{ image.alt }}"></a>
                            {% endif %}
                        {% endfor %}
                        </div>
                      {% else %}
                        <div class="bxslider-thumbs" id="bx-pager">
                          {% for image in product.images %}
                          <a data-slide-index="{{forloop.index0}}" href=""><img src="{{image.src | img_url: 'thumb'}}" alt="image-{{ image.alt }}"></a>
                          {% endfor %}
                        </div>
                      {% endif %}
                    </div>

                  </div>





            <!-- Product Info -->
            <div class="col-xs-12 col-md-5 col-lg-4 product-info">
              <h1>{{ product.title }}</h1>

              {% comment %}
                Sets boolean to reflect whether item is eligible for financing
              {% endcomment %}

              {% assign financing_eligible = false %}
                {% for collection in product.collections %}
                  {% if financing_eligible == false and collection.title == 'PP Collection' %}
                  	{% assign financing_eligible = true %}
                  {% endif %}
                {% endfor %}

              {% if financing_eligible %}
              <div class="finance-option-tooltip-wrapper">
                <span id="product-price" class="open-finance-option-tooltip product-price">{{ product.price | money }}</span>
                  <div id="finance-option-trigger">
                    Finance Options
                  </div>
                  <div class="finance-option-tooltip">
                    {% if cart.currency.iso_code == 'EUR' %}
                      This product has options for financing. <a href="/pages/payment-plan-product-page#product-{{ product.handle }}">Learn More</a>
                    {% else %}
                      This product has options for financing. <a href="/pages/payment-plan-landing-page">Learn More</a>
                    {% endif %}
                  </div>
                </div>
              {% else %}
              <span id="product-price" class="product-price">{{ product.price | money }}</span>
              {% endif %}

              <div class="product-option-icons">
                <!-- <div class="favourite" title="Save item" data-swaction="addToWishlist" data-product-id="{{product.id | json}}" ></div> -->
                <!-- <div class="iWishAdd favourite" data-product="14490304521" data-ptitle="Solid Gold Drop In The Wild Morganite Ring" title="Save item"></div> -->
                <!-- <div class="iwishAddWrap">
                    <a class="iWishAdd favourite" href="#" data-product="14490304521" data-ptitle="Solid Gold Drop In The Wild Morganite Ring"><i class="flaticon1-like50"></i> Add to Wishlist</a>
                    <p hidden class="iWishLoginMsg" style="display: none;">Your wishlist has been temporarily saved. Please <a href="/account/login">Log in</a> to save it permanently.</p>
                </div> -->
                <!-- Wishlist King May 18 -->
                <!-- include 'wishlist-button-product' with '{{ product.id }}' -->
                <div class="share" title="Share item">
                <ul class="share-list">
                  <!-- Share on Twitter -->
                  <li><a href="https://twitter.com/intent/tweet?text=I love this! What do you think? {{shop.domain}}{{product.url}}" onclick="window.open(this.href, 'mywin','left=50,top=50,width=600,height=350,toolbar=0'); return false;"><img src="{{ 'share-social-twitter.png' | asset_url }}" title="Share on Twitter"/></a></li>
                  <!-- Share on Facebook -->
                  <li><a href="http://www.facebook.com/sharer.php?u={{shop.domain}}{{product.url}}" onclick="window.open(this.href, 'mywin','left=50,top=50,width=600,height=350,toolbar=0'); return false;"><img src="{{ 'share-social-facebook.png' | asset_url }}" title="Share on Facebook"/></a></li>
                  <!-- Share on Pinterest -->
                  <li><a href="http://pinterest.com/pin/create/button/?url={{shop.url}}{{page.url}}&description=I%20love%20this%21%20What%20do%20you%20think%3F%20%2D%20{{ product.title }}&is_video=false&media={{ product.images[0] | product_img_url: 'large' }}" onclick="window.open(this.href, 'mywin','left=50,top=50,width=600,height=350,toolbar=0'); return false;"><img src="{{ 'share-social-pinterest.png' | asset_url }}" title="Share on Pinterest"/></a></li>
                  <!-- Share via email -->
                  <li><a href="mailto:?subject=I love this! What do you think?&amp;body=Hi%2C%0A%0AI%20love%20this!%20What%20do%20you%20think%3F%0A%0A%0A{{shop.domain}}{{product.url}}"><img src="{{ 'share-social-email.png' | asset_url }}" title="Share via email"/></a></li>
                  <!-- Share via WhatsApp -->
                  <li class="mobile-only"><a href="whatsapp://send?text=Hi%2C%0A%0AI%20love%20this!%20What%20do%20you%20think%3F%0A%0A%0A{{shop.url}}{{product.url}}" data-action="share/whatsapp/share"><img src="{{ 'share-social-whatsapp.png' | asset_url }}" title="Share on WhatsApp"/></a></li>
                </ul>
                </div>
                <!-- <div class="hint" title="Drop a hint"></div> -->
                <div class="hint" id="hint-app-open" title="Drop a hint"></div>
                <!-- Drop a hint script moved to bottom of template -->
                <!-- <a href="javascript:;" class="share drophint-link"></a> -->
              </div> <!-- end .product-option-icons -->

              {% if cart.currency.iso_code == 'USD' %}
                <span class="taxes-included-notice">(taxes included)</span>
              {% endif %}

              {% if financing_eligible %}
              	<p class="partial-payment-pushdown">
                  {% if product.type == 'Solid Gold' or product.type == 'Solid White Gold' or product.type == 'Solid Rose Gold' %}
                  Free worldwide delivery via DHL and 100 days for returns
                  {% else %}
                  Free worldwide delivery and 100 days for returns
                  {% endif %}
                </p>
              {% else %}
              	<p>
                  {% if product.type == 'Solid Gold' or product.type == 'Solid White Gold' or product.type == 'Solid Rose Gold' %}
                  Free worldwide delivery via DHL and 100 days for returns
                  {% else %}
                  Free worldwide delivery and 100 days for returns
                  {% endif %}
                </p>
              {% endif %}

              {% assign productTitle = product.title | downcase %}
              {% if productTitle contains 'coordinates' %}
              <!-- Select coordinates -->
              <div class="customise-field">
                <a class="select-coordinates" href="javascript:;" data-placeholder="Select coordinates...">Select coordinates...</a>
                <a href="javascript:;" class="add-remove"></a>
              </div>
              {% endif %}

              {% if product.metafields.customise.gemstone %}
              <!-- Select Birthstone -->
              <div class="customise-field">
                <a class="select-stone" href="javascript:;" data-placeholder="Select stone...">Select stone...</a>
                <a href="javascript:;" class="add-remove"></a>
              </div>
              {% endif %}
              {% if product.metafields.customise.solid_gold_gemstone %}
              <!-- Select Solid Gold Birthstone -->
              <div class="customise-field">
                <a class="select-solid-gold-stone" href="javascript:;" data-placeholder="Select stone...">Select stone...</a>
                <a href="javascript:;" class="add-remove"></a>
              </div>
              {% endif %}
              {% if product.metafields.customise.starsign %}
              <!-- Select Starsign -->
              {% if product.tags contains 'silver' or product.tags contains 'allsilver' or product.tags contains 'white gold' or product.tags contains 'solidWhiteGold' %}
              <div class="customise-field greyscale">
              {% else %}
              <div class="customise-field">
              {% endif %}
                <a class="select-starsign" href="javascript:;" data-placeholder="Select starsign...">Select starsign...</a>
                <a href="javascript:;" class="add-remove"></a>
              </div>
              {% endif %}
              {% if product.metafields.customise.starsign_symbol %}
              <!-- Select Starsign Symbol -->
              <div class="customise-field">
                <a class="select-starsign-symbol" href="javascript:;" data-placeholder="Select starsign...">Select starsign...</a>
                <a href="javascript:;" class="add-remove"></a>
              </div>
              {% endif %}
              {% if product.metafields.customise.zodiac %}
              <!-- Select Zodiac -->
              {% if product.tags contains 'silver' or product.tags contains 'allsilver' or product.tags contains 'white gold' or product.tags contains 'solidWhiteGold' %}
              <div class="customise-field greyscale">
              {% else %}
              <div class="customise-field">
              {% endif %}
                <a class="select-zodiac" href="javascript:;" data-placeholder="Select zodiac...">Select zodiac...</a>
                <a href="javascript:;" class="add-remove"></a>
              </div>
              {% endif %}
              {% if product.metafields.customise.initials %}
              <!-- Select Initials -->
              {% if product.tags contains 'silver' or product.tags contains 'allsilver' or product.tags contains 'white gold' or product.tags contains 'solidWhiteGold' %}
              <div class="customise-field greyscale">
              {% else %}
              <div class="customise-field">
              {% endif %}
                <a class="select-initials" href="javascript:;" data-placeholder="Select initials...">Select initials...</a>
                <a href="javascript:;" class="add-remove"></a>
              </div>
              {% endif %}
              {% assign productTitle = product.title | downcase %}
              {% if productTitle contains 'solid' or product.tags contains 'option-a-ring-sizes' or product.tags contains 'option-b-ring-sizes' %}
                {% if product.tags contains 'sgr' or product.tags contains 'SGR' or product.tags contains 'option-a-ring-sizes' or product.tags contains 'option-b-ring-sizes' %}
                  <!-- Select Ring size -->
                  <div class="customise-field">
                    <a class="select-ringsize sgr" href="javascript:;" data-placeholder="Select ring size...">Select ring size...</a>
                    <a href="javascript:;" class="add-remove"></a>
                  </div>
                {% elsif product.options contains "Size" %}
                <!-- Select Ring size -->
                  <div class="customise-field">
                    <a class="select-ringsize" href="javascript:;" data-placeholder="Select ring size...">Select ring size...</a>
                    <a href="javascript:;" class="add-remove"></a>
                  </div>
                {% endif %}
              {% else %}
                {% if product.options contains "Size" %}
                  <!-- Select Ring size -->
                  <div class="customise-field">
                    <a class="select-ringsize" href="javascript:;" data-placeholder="Select ring size...">Select ring size...</a>
                    <a href="javascript:;" class="add-remove"></a>
                  </div>
                {% endif %}
              {% endif %}




              {% if product.tags contains 'secret' %}
              <a href="javascript:;" class="add-to-cart add-button add-button-commit"><span>Add to bag</span></a>
              {% else %}
              <a href="javascript:;" class="add-to-cart add-button"><span>Add to bag</span></a>
              {% endif %}



              <!-- Hint Overlay Panel -->
              <div class="customise-overlay hint-overlay">
                <div class="inner">

                  <a href="javascript:;" class="overlay-close hint-close"></a>

                  <div class="middle">

                    <div>
                      <form data-toggle="validator" id="dropaform" role="form">
                        <h3>Drop a hint to:</h3>
                        <div class="form-group">
                          <input type="text" class="form-control recName" name="recName" id="hint-recipient-name" placeholder="Their name" data-error="Please provide a name for the recipient" required>
                          <div class="help-block with-errors"></div>
                        </div>
                        <div class="form-group">
                          <input type="text" class="form-control recEmail" name="recEmail" id="hint-recipient-email" placeholder="Their email address" data-error="Please provide a valid email address" required>
                          <div class="help-block with-errors"></div>
                        </div>
                        <h3>Sent from:</h3>
                        <div class="form-group">
                          <input type="text" class="form-control senderName" name="senderName" id="hint-sender-name" placeholder="Your name" data-error="Please provide your name" required>
                          <div class="help-block with-errors"></div>
                        </div>
                        <div class="form-group">
                          <input type="text" class="form-control senderEmail" name="senderEmail" id="hint-sender-email" placeholder="Your email address" data-error="Please provide a valid email address" required>
                          <div class="help-block with-errors"></div>
                        </div>
                        <h3>Anything else:</h3>
                        <div class="form-group">
                          <textarea class="form-control" id="hint-sender-extra" placeholder="You can pop any extra details such as your ring size in here"></textarea>
                          <div class="help-block with-errors"></div>
                        </div>
                        <div class="form-group clear-float">
                          <input type="checkbox" name="" id="hint-sign-up" />
                          <!-- <div class="checkbox-box"></div> -->
                          <label for="hint-sign-up">I would love to receive the wonderful Chupi newsletter</label>
                        </div>
                        <div class="form-group">
                          <button type="submit" class="btn btn-primary full-width">Send my hint</button>
                        </div>
                      </form>
                    </div>

                    <div id="hint-email">
                      <img src="{{ 'paper-plane.png' | asset_url }}"/>
                      <h3>Drop a hint</h3>
                      <p>Dear <span class="name-space name-space-recipient"></span>.</p>
                      <p>We happen to know that</p>

                      {% assign image_count = 0 %}
                      {% for image in product.images %}
                        <div class="hint-product-image" style="background-image: url('{{image.src | img_url: '1024x1024'}}');"></div>
                        {% assign image_count = image_count | plus: 1 %}
                        {% if image_count == 1 %}{% break %}{% endif %}
                      {% endfor %}

                      <span class="hint-product-title">{{ product.title }}</span>
                      <p>would make <span class="name-space name-space-sender"></span> ridiculously happy!</p>
                    </div>

                  </div> <!-- end .middle -->

                </div> <!-- end .inner -->
              </div> <!-- end .hint-overlay -->




              <!--================ Customise Overlay Panels ================-->


              <!-- Add-to-bag overlay (master-panel) -->
              <div class="customise-overlay add-overlay">
                <div class="inner">

                  <div class="top">
                    <a href="javascript:;" class="overlay-close add-close"></a>
                    <h2>Make it yours</h2>
                  </div>

                  <div class="middle">

                    <!-- Fields can have optional 'required-field' class -->

                    {% if product.metafields.customise.gemstone %}
                    <!-- Select stone -->
                    <label for="select-stone">Choose your gemstone</label>
                    <a class="select-stone" href="javascript:;">Select...</a>
                    {% endif %}
                    {% if product.metafields.customise.solid_gold_gemstone %}
                    <!-- Select Solid Gold stone -->
                    <label for="select-solid-gold-stone">Choose your gemstone</label>
                    <a class="select-solid-gold-stone" href="javascript:;">Select...</a>
                    {% endif %}
                    {% if product.metafields.customise.starsign %}
                    <!-- Select star sign -->
                    <label for="select-starsign">Choose your star sign</label>
                    <a class="select-starsign required-field" href="javascript:;">Select...</a>
                    {% endif %}
                    {% if product.metafields.customise.starsign_symbol %}
                    <!-- Select star sign symbol -->
                    <label for="select-starsign-symbol">Choose your star sign</label>
                    <a class="select-starsign-symbol required-field" href="javascript:;">Select...</a>
                    {% endif %}
                    {% if product.metafields.customise.zodiac %}
                    <!-- Select zodiac -->
                    <label for="select-zodiac">Choose your zodiac</label>
                    <a class="select-zodiac required-field" href="javascript:;">Select...</a>
                    {% endif %}
                    {% if product.metafields.customise.initials %}
                    <!-- Select initials -->
                    <label for="select-initials">Choose your letter</label>
                    <a class="select-initials required-field" href="javascript:;">Select...</a>
                    {% endif %}
                    {% if product.options contains "Size" %}
                    <!-- Select ring size -->
                    <label for="select-ringsize">Select ring size</label>
                    <a class="select-ringsize required-field" href="javascript:;">Select...</a>
                    {% endif %}

                    <!-- Additional notes -->
                    <div class="notes-wrapper">
                    <label for="additional-notes">WOULD YOU LIKE TO ADD A NOTE?</label>
                    <textarea id="note" class="additional-notes" maxlength="150"></textarea>

                      <div id="additional-notes-progress" title="As our notes are handwritten, they are limited to 150 characters">
                        <svg class="progress-radial" width="24" height="24" viewBox="0 0 120 120">
                          <circle class="progress-radial__meter" cx="60" cy="60" r="54" stroke-width="12" />
                          <circle class="progress-radial__value" cx="60" cy="60" r="54" stroke-width="12" stroke-dasharray="339.292" stroke-dashoffset="339.292" />
                        </svg>
                      </div>
                    </div>
					<!--29/08/17 added in hidden field for add to cart -->
                    <input type="hidden" id="selected-variant-id" value="{%if number_of_variants == "1" %}{{product.variants[0].id}}{%else%}{{product.variants[0].id}}{%endif%}" />
                  </div>

                  <div class="bottom">
                    <a href="javascript:;" class="add-to-cart add-button-commit">Add to bag</a>
                  </div>

                </div> <!-- end .inner -->
              </div> <!-- end .add-overlay -->

              <!-- Coordinates overlay -->
              <div class="customise-overlay coordinates-overlay">
                <div class="inner">

                  <div class="top">
                    <a href="javascript:;" class="overlay-close coordinates-close"></a>
                    <h3>Select coordinates</h3>
                    <a class="info" href="https://blog.chupi.com/how-to-find-your-coordinates/" target="_blank">Not sure how to pick your coordinates?</a>
                  </div>

                  <div class="middle">
                    <div class="inner">

                      <form class="coordinates-form">
                        <div class="northSouth form-group">
                          <input type="tel" min="0" max="90" class="degrees" id="ns-degrees" /><label for="ns-degrees">°</label>
                          <input type="tel" min="0" max="60" class="minutes" id="ns-minutes" /><label for="ns-minutes">'</label>
                          <input type="hidden" maxlength="1" class="northSouthValue" value="n">
                          <div class="switch" data-options="n-s">
                            <span class="switch-north">N</span>
                            <span class="switch-south">S</span>
                          </div>
                        </div>
                        <div class="eastWest form-group">
                          <input type="tel" min="0" max="90" class="degrees" id="ew-degrees" required /><label for="ew-degrees">°</label>
                          <input type="tel" min="0" max="60" class="minutes"  id="ew-minutes" required /><label for="ew-minutes">'</label>
                          <input type="hidden" maxlength="1" class="eastWestValue" value="e">
                          <div class="switch" data-options="e-w">
                            <span class="switch-east">E</span>
                            <span class="switch-west">W</span>
                          </div>
                        </div>
                      </form>
                      <span class="coordinates-error-msg">No Errors</span>
                      <div class="coordinates-preview">
                        <h3 class="preview">
                          <span class="lead-in">
                            Your Coordinates
                          </span>
                          <span class="ns">
                            00°00'N
                          </span>
                          <span class="ew">
                            00°00'E
                          </span>
                        </h3>
                      </div>
                    </div> <!-- end inner -->
                  </div> <!-- middle / stones-all -->
                  <div class="bottom">
                    <a href="javascript:;" class="apply-coordinates disabled">Apply</a>
                  </div>
                </div> <!-- end inner -->
              </div> <!-- end .coordinates-overlay -->


              <!-- Birthstones overlay -->
              <div class="customise-overlay stones-overlay">
                <div class="inner">

                  <div class="top">
                    <a href="javascript:;" class="overlay-close stones-close"></a>
                    <h3>Select birthstone</h3>
                  </div>

                  <div class="middle stones-all">
                    <div class="inner">

                      <!-- No birthstone -->
                      <a href="javascript:;" class="stone-none"><span>Classic necklace with no birthstone</span></a>

